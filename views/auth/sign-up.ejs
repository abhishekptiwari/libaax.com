    <!--=================Account Section================= -->
    <section class="account-section padding-top padding-bottom bg_img bg_right_bottom"
        data-background="./assets/images/account/account-bg.png">
        <div class="container">
            <div class="account-wrapper">
                <div class="account-area">
                    <h3 class="account-title">create new account</h3>
                    <form class="sign-in-form" method="POST" action="/sign-up">
                        <% if (errorMsg) { %> 
                            <% if(errorMsg.length === 4) { %>
                            <div class="alert alert-dismissible alert-danger">
                                <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                <strong><%= errorMsg[0].msg %></strong>
                            </div>
                            <% } else if(errorMsg.length === 3  && errorMsg[0].param === 'fname') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 2  && errorMsg[0].param === 'fname') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 1  && errorMsg[0].param === 'lname') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } %>
                        <% } %>
                        <div class="form-group">
                            <input type="text" value="<%= oldInput.fname %>" placeholder="First Name" name="fname">
                        </div>
                        <% if (errorMsg) { %> 
                            <% if(errorMsg.length === 4) { %>
                            <div class="alert alert-dismissible alert-danger">
                                <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                <strong><%= errorMsg[1].msg %></strong>
                            </div>
                            <% } else if(errorMsg.length === 3  && (errorMsg[0].param === 'lname' || errorMsg[1].param === 'lname')) { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 2  && (errorMsg[0].param === 'lname' || errorMsg[1].param === 'fname')) { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 1  && errorMsg[0].param === 'lname') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } %>
                        <% } %>
                        <div class="form-group">
                            <input type="text" value="<%= oldInput.lname %>" placeholder="Last Name" name="lname">
                        </div>
                        <% if (errorMsg) { %> 
                            <% if(errorMsg.length === 4) { %>
                            <div class="alert alert-dismissible alert-danger">
                                <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                <strong><%= errorMsg[2].msg %></strong>
                            </div>
                            <% } else if(errorMsg.length === 3  && (errorMsg[2].param === 'email' || errorMsg[1].param === 'email' )) { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <% let emailMsg = errorMsg[2].param === 'email' ? errorMsg[2].msg : errorMsg[1].msg %>
                                    <strong><%= emailMsg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 2  && (errorMsg[1].param === 'email' || errorMsg[0].param === 'email')) { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <% let emailMsg = errorMsg[1].param === 'email' ? errorMsg[1].msg : errorMsg[0].msg %>
                                    <strong><%= emailMsg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 1  && errorMsg[0].param === 'email') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <%}%>
                        <% } %>
                        <div class="form-group">
                            <input type="text" placeholder="Email" value="<%= oldInput.email %>" name="email">
                        </div>
                        <% if (errorMsg) { %> 
                            <% if(errorMsg.length ===4) { %>
                            <div class="alert alert-dismissible alert-danger">
                                <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                <strong><%= errorMsg[3].msg %></strong>
                            </div>
                            <% } else if(errorMsg.length === 3  && errorMsg[2].param === 'password') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[2].msg %></strong>
                                </div>
                            <% } else if(errorMsg.length === 2  && errorMsg[1].param === 'password') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[1].msg %></strong>
                                </div>
                            <% }else if(errorMsg.length === 1  && errorMsg[0].param === 'password') { %>
                                <div class="alert alert-dismissible alert-danger">
                                    <button type="button" class="close back" data-dismiss="alert">&times;</button>
                                    <strong><%= errorMsg[0].msg %></strong>
                                </div>
                            <% } %>
                        <% } %>
                        <div class="form-group">
                            <input type="password" name="password" value="<%= oldInput.password %>" placeholder="Password" id="my-input-01">
                            <span class="view-pass" id="view-pass-01">
                                <i class="flaticon-eye"></i>
                            </span>
                        </div>
                        <div class="form-group">
                            <input name="_csrf" type="hidden" value="<%= csrfToken %>" />
                            <input type="submit" value="Create New Account">
                        </div>
                    </form>
                    <div class="forget-pass-group d-flex flex-wrap justify-content-between">
                        <div class="forget-pass">
                            <p><a href="#0">forget password</a></p>
                        </div>
                        <div class="no-account">
                            <p>Already a user? <a href="/sign-in">Sign-In</a></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
  
